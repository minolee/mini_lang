declaration: a, b, c, d, e

a := random(10)
b := random(10)
c := random(10)
d := random(10)
e := random(10)

print(a)
print(b)
print(c)
print(d)
print(e)

do
    a > b -> a, b := b, a
|   b > c -> b, c := c, b
|   c > d -> c, d := d, c
|   d > e -> d, e := e, d
od

print()

print(a)
print(b)
print(c)
print(d)
print(e)